@article{campedelliMultimodalNetworksReveal2021,
  title = {Multi-Modal {{Networks Reveal Patterns}} of {{Operational Similarity}} of {{Terrorist Organizations}}},
  author = {Campedelli, Gian Maria and Cruickshank, Iain and Carley, Kathleen M.},
  date = {2021-12-14},
  journaltitle = {Terrorism and Political Violence},
  volume = {0},
